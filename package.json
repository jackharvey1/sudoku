{
  "name": "sudoku",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "emotion": "^9.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^9.1.3",
    "react-icons": "^2.2.7",
    "react-scripts": "1.1.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src/",
    "jest": "react-scripts test --env=jsdom",
    "test": "npm run lint && npm run jest",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "codecov": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-react": "^7.7.0"
  }
}
